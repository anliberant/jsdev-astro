  fill='none'
  stroke='currentColor'
  stroke-width='2'
  stroke-linecap='round'
  stroke-linejoin='round'
  class={className}>
  <rect
    x='2'
    y='3'
    width='20'
    height='14'
    rx='2'
    ry='2'
  ></rect>
  <rect
    x='6'
    y='7'
    width='12'
    height='6'
    rx='1'
    ry='1'
    fill='currentColor'
    opacity='0.3'
  ></rect>
  <path d='m7 20 5-5 6 5'></path>
  <path d='M4 6h2m6 0h2m6 0h2'></path>
</svg>
